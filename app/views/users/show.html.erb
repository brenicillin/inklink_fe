<h1>User Dashboard</h1>

<h3>Search for Shops</h3>
<%= form_with url: "/artists", method: :get, local: true do |form| %>
  <%= form.label :city, "City" %>
  <%= form.text_field :city %>
  <%= form.submit "Start Search" %>
<% end %>

<h3>Discover Tattoos</h3>
<%= form_with url: user_discover_index_path(@_current_user), method: :get, local: true do |form| %>
  <%= form.label :styles %><br>
  <%= form.select :styles, ["Watercolor", "American Traditional", "Geometric", "Script", "Black and Gray", "Realism", "Tribal", "Chicano", "Irezumi", "New School"] %>
  <%= form.submit "Search" %>
<% end %>